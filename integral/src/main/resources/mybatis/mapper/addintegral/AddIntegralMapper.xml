<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.integral.base.addintegral.dao.AddIntegralDao">

    <insert id="addIntegral" parameterType="com.neusoft.integral.base.addintegral.entity.IntegralEntity">
        INSERT INTO
          dis_integral
        (
          id,creater,create_time,order_id
        )
        VALUES
        (
          #{id},#{creater},#{createTime},#{orderId}
        )
    </insert>
</mapper>